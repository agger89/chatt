{
  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true,
    "lib": ["ES2020", "DOM"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "Node",
    "target": "es5",
    "strict": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "types": ["@emotion/react/types/css-prop"],
    "paths": {
      "hooks/*": ["hooks/*"],
      "components/*": ["components/*"],
      "layouts/*": ["layouts/*"],
      "pages/*": ["pages/*"],
      "utils/*": ["utils/*"],
      "typings/*": ["typings/*"]
    }
  }
}
